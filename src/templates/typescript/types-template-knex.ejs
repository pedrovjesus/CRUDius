<%# path: database/knex/@types %>
<% entities.forEach(entity => { %>
import { <%= entity.name %> } from "../../entities/<%= entity.name %>.model";
<% }) %>

declare module "knex/types/tables" {
interface Tables {
<% entities.forEach(entity => { %>
<%= entity.name.toLowerCase() %>: <%= entity.name %>;
<% }) %>
}
}